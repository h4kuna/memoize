{
  "name": "h4kuna/memoize",
  "description": "Provide cache to memory for your class.",
  "type": "tool",
  "license": "MIT",
  "authors": [
    {
      "name": "Milan Matějček",
      "email": "milan.matejcek@gmail.com"
    }
  ],
  "require-dev": {
    "nette/tester": "^2.0",
    "phpstan/phpstan": "^1.9"
  },
  "scripts": {
    "phpstan": "./vendor/bin/phpstan analyse -l max src/",
    "tests": "vendor/bin/tester -s -j 4 --colors 1 -s -C tests"
  },
  "autoload": {
    "psr-4": {
      "h4kuna\\Memoize\\": "src/"
    }
  }
}
